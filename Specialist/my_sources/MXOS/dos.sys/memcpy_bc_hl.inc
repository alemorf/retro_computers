; Скопировать блок памяти
;
; На входе
;  hl - откуда начало
;  de - откуда конец + 1
;  (de-hl) - длина
;  bc - куда
;
; На выходе
;  de - сохраняется
;
; 2013-12-12 Дизассемблировано vinxru
;----------------------------------------------------------------------------

memcpy_bc_hl:	mov	a, m
		stax	b
		inx	h
		inx	b
		call	cmp_hl_de
		jnz	memcpy_bc_hl
		ret